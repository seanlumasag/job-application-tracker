{
"compilerOptions": {
  "target": "ES2022",
  "lib": ["ES2022", "DOM", "DOM.Iterable"],
  "module": "ESNext",
  "moduleResolution": "Bundler",
  "jsx": "react-jsx",
  "noEmit": true,
  "skipLibCheck": true,
  "types": ["node"],
  "typeRoots": ["./node_modules/@types"],
  "baseUrl": ".",
  "paths": {
    "@/*": ["src/*"]
  }
},
"include": ["src", "vite.config.ts"]
}
